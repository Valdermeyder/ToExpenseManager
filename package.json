    "csv-parse": "^4.15.3",
    "express": "^4.18.2",
    "express-fileupload": "^1.1.6",
    "moment": "^2.29.4",
    "stream-transform": "^2.0.1"
  },
  "devDependencies": {
    "eslint": "^7.23.0",
    "eslint-config-esnext": "^4.0.0",
    "eslint-config-node": "^4.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^24.3.2",
    "husky": "^7.0.0",
    "jest": "^27.2.0",
    "lodash": "^4.17.21",
    "testcafe": "^1.13.0"
  }
