{
  "name": "citi_to_expense_manager_csv_converter",
  "version": "1.0.0",
  "description": "Convert CSV files from Citi to Expense Manager format",
  "repository": {
    "type": "git",
    "url": "https://github.com/Valdermeyder/CityToExpenseManager.git"
  },
  "main": "main.js",
  "scripts": {
    "start": "node main.js",
    "lint": "eslint .",
    "test": "jest",
    "e2e": "testcafe chrome ./e2e/*.js --app \"yarn start\"",
    "e2e:ci": "testcafe chrome:headless ./e2e/*.js --app \"yarn start\"",
    "deploy": "git push heroku master",
    "prepare": "husky install"
  },
  "keywords": [
    "csv",
    "expense",
    "converter"
  ],
  "author": "Volodymyr Huzar",
  "email": "vguzar@i.ua",
  "license": "ISC",
